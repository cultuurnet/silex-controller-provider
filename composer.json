{
    "name": "cultuurnet/silex-controller-provider-entryapi",
    "description": "silex app to run the controller providers for the entryapi",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
		"silex/silex": "~1.3"
	},
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "phpunit/php-token-stream": "1.4.6",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
