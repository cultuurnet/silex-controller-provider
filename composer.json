{
    "name": "cultuurnet/udb3-silex-entryapi",
    "description": "EntryAPI for UDB3 Silex",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "silex/silex": "~1.3",
        "cultuurnet/cdb": "~2.1",
        "cultuurnet/udb3": "dev-feature/III-481 as 0.1",
        "cultuurnet/entry": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "phpunit/php-token-stream": "1.4.6",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3SilexEntryAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3SilexEntryAPI\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
